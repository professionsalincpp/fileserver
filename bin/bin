import hashlibimport osimport requestsimport argparsefrom colorama import Fore, Stylefrom logger import MultiLogger_logger = MultiLogger("client")# add subcommand [upload] to upload file to serverdef parse_args():    parser = argparse.ArgumentParser(description="File Server")    # add subcommand upload    upload_command = parser.add_subparsers(description="Upload file to server", dest="upload")      import argparsedef parse_args():    parser = argparse.ArgumentParser(description="File Server")    subparsers = parser.add_subparsers(dest="subcmd")    upload_parser = subparsers.add_parser("upload", help="Upload file to server")    upload_parser.add_argument("file", type=str, help="File to upload")    return parser.parse_args()def upload_file(file_path):    if not os.path.exists(file_path):        _logger.error(f"File \"{file_path}\" does not exist", context="upload_file")        return    with open(file_path, "r") as f:        data = f.read()        file_hash = hashlib.sha256(data.encode()).hexdigest()        to_post = {            "path": file_path.split("/")[-1],            "data": data,            "hash": file_hash        }        response: requests.Response = requests.post(url="http://192.168.1.44:8080", headers={"Content-Type": "application/json", "Content-Length": str(len(data))}, json=to_post)        if response.status_code == 200:            _logger.success(f"File \"{file_path}\" uploaded successfully", context="upload_file")            print(f" {Fore.WHITE}~{Style.RESET_ALL} File hash: {file_hash}")        else:            _logger.error(f"File \"{file_path}\" upload failed", context="upload_file")            if response.status_code == 400:                _logger.error(response.text, context="")            def main():    args = parse_args()    if args.subcmd == "upload":        if args.file:            upload_file(args.file)            # data = "Hello, World!"# to_post = {#     "path": "hello.txt",#     "data": data,#     "hash": hashlib.sha256(data).hexdigest()# }# print(requests.post(url="http://192.168.1.44:8080", headers={"Content-Type": "application/json", "Content-Length": str(len(b"Hello, World!"))}, json=to_post).text)if __name__ == "__main__":    main()